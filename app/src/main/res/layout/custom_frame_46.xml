<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameNew"
    android:layout_width="match_parent"
    android:layout_height="wrap_content">

    <ImageView
        android:id="@+id/ivframebg"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_300sdp"
        android:background="@drawable/frame_46"
        android:orientation="vertical" />

    <FrameLayout
        android:id="@+id/frameLogo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_gravity="top|start"
        android:layout_margin="@dimen/_2sdp"
        android:visibility="visible">

        <LinearLayout
            android:id="@+id/linearLogo"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="right"
            android:layout_margin="@dimen/_2sdp"
            android:visibility="visible">

            <ImageView
                android:id="@+id/ivframelogo"
                android:layout_width="@dimen/_60sdp"
                android:layout_height="@dimen/_60sdp"
                android:layout_gravity="left|center"
                android:layout_margin="@dimen/_2sdp" />

        </LinearLayout>

        <ImageView
            android:id="@+id/imgLogoClose"
            android:layout_width="@dimen/_16sdp"
            android:layout_height="@dimen/_16sdp"
            android:layout_gravity="top|start"
            android:elevation="1dp"
            android:src="@drawable/ic_remove"
            android:visibility="gone" />

    </FrameLayout>


    <FrameLayout
        android:id="@+id/frameName"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:visibility="gone"
        tools:visibility="visible"
        android:layout_gravity="top|end"
        android:layout_marginTop="@dimen/_10sdp"
        android:layout_marginRight="@dimen/_15sdp"
        >

        <LinearLayout
            android:id="@+id/linearName"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone"
            tools:visibility="visible"
            android:layout_marginLeft="@dimen/_2sdp"
            android:background="@drawable/rounded_border_tv_new"
            android:orientation="horizontal">


            <TextView
                android:id="@+id/tvframename"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_2sdp"
                android:ellipsize="end"
                android:gravity="center"
                android:maxLines="1"
                android:textStyle="bold"
                tools:text="fsdfdsfdsfdsfdsfds"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/_8sdp" />
        </LinearLayout>

        <ImageView
            android:id="@+id/imgNameClose"
            android:layout_width="@dimen/_10sdp"
            android:layout_height="@dimen/_10sdp"
            android:layout_gravity="top|start"
            android:elevation="1dp"
            android:src="@drawable/ic_remove"
            android:visibility="gone" />

    </FrameLayout>


<LinearLayout
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_gravity="bottom|center"
    android:gravity="center"
    android:layout_marginLeft="@dimen/_35sdp"
    android:layout_marginRight="@dimen/_35sdp"
    android:layout_marginBottom="@dimen/_10sdp"
    >

        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:orientation="vertical">


            <FrameLayout
                android:id="@+id/framePhone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center"
                android:layout_marginBottom="@dimen/_15sdp"
                android:visibility="visible">


                <LinearLayout
                    android:id="@+id/linearPhone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:background="@drawable/rounded_border_tv_new"
                    android:gravity="center"
                    tools:visibility="visible"
                    android:orientation="horizontal"
                    android:visibility="gone">


                    <LinearLayout
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="visible"
                        android:gravity="center"
                        tools:visibility="visible"

                        >

                        <ImageView
                            android:id="@+id/ivPhone"
                            android:layout_width="10dp"
                            android:layout_height="10dp"
                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:background="@drawable/ic_call_46"
                            android:visibility="gone"
                            tools:visibility="visible" />


                        <TextView
                            android:id="@+id/tvframephone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_5sdp"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:maxLines="1"
                            android:textColor="@color/colorWhite"
                            tools:visibility="visible"
                            tools:text="1234567890"
                            android:textSize="@dimen/_8sdp"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/viewPhone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:text="|"
                            android:textColor="@color/colorBlack"
                            android:textSize="@dimen/_6sdp"
                            android:visibility="gone" />
                    </LinearLayout>

                    <LinearLayout

                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="visible"

                        >

                        <ImageView
                            android:id="@+id/ivPhone1"
                            android:layout_width="@dimen/_7sdp"
                            android:layout_height="@dimen/_8sdp"

                            android:layout_gravity="center"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:background="@drawable/ic_call_orange"
                            android:backgroundTint="@color/white"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/tvframephone1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:ellipsize="end"
                            android:gravity="center"
                            android:maxLines="1"

                            android:textColor="@color/white"
                            android:textSize="@dimen/_8sdp"
                            android:visibility="gone" />

                        <TextView
                            android:id="@+id/viewPhone1"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_marginLeft="@dimen/_2sdp"
                            android:text="|"
                            android:textColor="@color/colorBlack"
                            android:textSize="@dimen/_6sdp"
                            android:visibility="gone" />
                    </LinearLayout>
                </LinearLayout>

                <ImageView
                    android:id="@+id/imgPhoneClose"
                    android:layout_width="@dimen/_8sdp"
                    android:layout_height="@dimen/_8sdp"
                    android:layout_gravity="top|start"
                    android:elevation="1dp"
                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />

            </FrameLayout>



        </LinearLayout>
        <LinearLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:visibility="visible"
            android:layout_gravity="bottom|center"
            android:layout_marginRight="@dimen/_5sdp"
            android:layout_marginLeft="@dimen/_5sdp"
            android:gravity="center"
            android:layout_marginBottom="@dimen/_15sdp"
            >

            <FrameLayout
                android:id="@+id/frameEmail"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="gone"
                android:layout_gravity="center"
                tools:visibility="visible"

                >

                <LinearLayout
                    android:id="@+id/linearEmail"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:background="@drawable/rounded_border_tv_new"
                    android:orientation="horizontal"
                    tools:visibility="visible"
                    android:visibility="gone">

                    <ImageView
                        android:id="@+id/ivEmail"
                        android:layout_width="@dimen/_7sdp"
                        android:layout_height="@dimen/_7sdp"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/_2sdp"
                        android:layout_marginTop="2px"
                        android:visibility="gone"
                        android:background="@drawable/ic_email_46"
                        tools:visibility="visible"
                        />

                    <TextView
                        android:id="@+id/tvframeemail"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_marginLeft="@dimen/_5sdp"
                        android:ellipsize="end"
                        android:gravity="center"
                        android:maxLines="1"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/_8sdp"
                        android:visibility="gone"
                        tools:text="ganehs@gmail.com"
                        tools:visibility="visible" />

                    <TextView
                        android:id="@+id/viewwebsite"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_2sdp"
                        android:text="|"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_6sdp"
                        android:visibility="gone" />
                </LinearLayout>

                <ImageView
                    android:id="@+id/imgEmailClose"
                    android:layout_width="@dimen/_8sdp"
                    android:layout_height="@dimen/_8sdp"
                    android:layout_gravity="top|start"
                    android:elevation="1dp"

                    android:src="@drawable/ic_remove"
                    android:visibility="gone" />

            </FrameLayout>




        </LinearLayout>
    <LinearLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:orientation="horizontal"
        android:visibility="visible"
        tools:visibility="visible"
        android:layout_gravity="center"
        android:layout_marginBottom="@dimen/_7sdp"
        >

        <FrameLayout
            android:id="@+id/frameWebsite"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:gravity="center"
            tools:visibility="visible"
            android:visibility="gone">

            <LinearLayout
                android:id="@+id/linearWebsite"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:background="@drawable/rounded_border_tv_new"
                tools:visibility="visible"
                android:layout_gravity="end"
                android:gravity="center"
                android:orientation="horizontal"
                android:visibility="gone">

                <ImageView
                    android:id="@+id/ivWebsite"
                    android:layout_width="@dimen/_8sdp"
                    android:layout_height="@dimen/_8sdp"
                    android:layout_gravity="center"
                    android:layout_marginLeft="@dimen/_2sdp"
                    android:background="@drawable/ic_website_46"
                    android:visibility="gone"
                    tools:visibility="visible" />

                <TextView
                    android:id="@+id/tvframeweb"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="@dimen/_5sdp"
                    android:ellipsize="end"
                    android:gravity="center"
                    android:maxLines="1"
                    tools:visibility="visible"
                    android:visibility="gone"
                    tools:text="www.ganesh.com"
                    android:textColor="@color/colorWhite"
                    android:textSize="@dimen/_8sdp" />

            </LinearLayout>

            <ImageView
                android:id="@+id/imgWebsiteClose"
                android:layout_width="@dimen/_8sdp"
                android:layout_height="@dimen/_8sdp"
                android:layout_gravity="top|start"
                android:elevation="1dp"
                android:src="@drawable/ic_remove"

                android:visibility="gone" />

        </FrameLayout>
    </LinearLayout>
</LinearLayout>



