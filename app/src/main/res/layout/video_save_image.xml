<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    >

        <FrameLayout
            android:id="@+id/frame"
            android:layout_width="1080px"
            android:layout_height="1080px">





                    <ImageView
                        android:id="@+id/ivframelogo1"
                        android:layout_width="100dp"
                        android:layout_gravity="right"
                        android:layout_height="80dp"
                        android:layout_marginTop="@dimen/_5sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:visibility="visible"
                        />


            <FrameLayout
                android:id="@+id/frameName"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:visibility="visible"
                android:layout_gravity="bottom|center"
                android:layout_marginBottom="@dimen/_20sdp"

                >

                <LinearLayout
                    android:id="@+id/linearName"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:visibility="visible"
                    android:layout_gravity="bottom"
                    android:gravity="bottom"


                    android:orientation="horizontal">


                    <TextView
                        android:id="@+id/tvframename"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:ellipsize="end"
                        android:textColor="@color/colorBlack"
                        android:layout_gravity="bottom"
                        android:gravity="bottom"
                        android:maxLines="1"
                        android:visibility="gone"
                        android:textSize="@dimen/_8sdp"
                        android:textStyle="bold"

                        />
                </LinearLayout>

                <ImageView
                    android:id="@+id/imgNameClose"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="top|start"
                    android:elevation="1dp"

                    android:visibility="visible" />

            </FrameLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_gravity="bottom|center"
                android:gravity="center"
                android:layout_marginBottom="@dimen/_3sdp"
                android:orientation="vertical">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <FrameLayout
                        android:id="@+id/framePhone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="visible"
                        >

                        <LinearLayout
                            android:id="@+id/linearPhone"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"

                            android:visibility="visible"
                            >

                            <ImageView
                                android:id="@+id/ivPhone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                />

                            <TextView
                                android:id="@+id/tvframephone"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textSize="@dimen/_7sdp"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:maxLines="1"
                                android:visibility="gone"
                                android:text="7686894444"
                                />

                        </LinearLayout>


                    </FrameLayout>

                    <TextView
                        android:id="@+id/viewPhone"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_2sdp"
                        android:layout_marginRight="@dimen/_2sdp"
                        android:visibility="gone"
                        android:text="|"
                        android:textSize="@dimen/_7ssp"

                        />



                    <FrameLayout
                        android:id="@+id/frameEmail"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="visible"
                        >

                        <LinearLayout
                            android:id="@+id/linearEmail"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"


                            android:visibility="visible"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                />

                            <TextView
                                android:id="@+id/tvframeemail"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:textColor="@color/colorBlack"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:maxLines="1"
                                android:layout_marginLeft="@dimen/_2sdp"
                                android:visibility="gone"
                                android:textSize="@dimen/_7sdp"
                                android:text="bapa.infodesign@gmail.com"
                                />
                        </LinearLayout>

                        <ImageView
                            android:id="@+id/imgEmailClose"
                            android:layout_width="5dp"
                            android:layout_height="5dp"
                            android:layout_gravity="top|start"
                            android:elevation="1dp"

                            android:visibility="visible" />

                    </FrameLayout>

                    <TextView
                        android:id="@+id/viewwebsite"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_2sdp"
                        android:layout_marginRight="@dimen/_2sdp"
                        android:visibility="visible"
                        android:text="|"
                        android:textSize="@dimen/_7ssp"

                        />

                    <FrameLayout
                        android:id="@+id/frameWebsite"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="visible"
                        >

                        <LinearLayout
                            android:id="@+id/linearWebsite"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"


                            android:visibility="visible"
                            android:orientation="horizontal">

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"
                                />

                            <TextView
                                android:id="@+id/tvframeweb"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:visibility="gone"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:maxLines="1"
                                android:textSize="@dimen/_7sdp"
                                android:layout_marginLeft="@dimen/_2sdp"
                                android:text="bapa.infodesign@gmail.com"
                                />

                        </LinearLayout>


                    </FrameLayout>


                </LinearLayout>


                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <FrameLayout
                        android:id="@+id/frameLocation"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="visible"
                        >

                        <LinearLayout
                            android:id="@+id/linearLocation"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"


                            android:orientation="horizontal"
                            android:visibility="visible"
                            >

                            <ImageView
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:layout_gravity="center"

                                />

                            <TextView
                                android:id="@+id/tvframelocation"
                                android:layout_width="wrap_content"
                                android:layout_height="wrap_content"
                                android:ellipsize="end"
                                android:gravity="center"
                                android:visibility="gone"
                                android:maxLines="1"
                                android:textSize="@dimen/_7sdp"
                                android:textColor="@color/colorBlack"
                                android:text="Madhav Park, Punit Nagar, Mavdi, Rajkot, Gujarat 360004"
                                />

                        </LinearLayout>

                        <ImageView
                            android:id="@+id/imgLocationClose"
                            android:layout_width="5dp"
                            android:layout_height="5dp"
                            android:layout_gravity="top|start"
                            android:elevation="1dp"
                            android:visibility="gone" />

                    </FrameLayout>


                </LinearLayout>


            </LinearLayout>

        </FrameLayout>




</LinearLayout>