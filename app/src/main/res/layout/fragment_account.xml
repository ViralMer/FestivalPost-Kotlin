<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackground"
    android:orientation="vertical">

    <androidx.appcompat.widget.Toolbar
        android:id="@+id/toolbar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="@dimen/_5sdp">

            <TextView
                android:id="@+id/tvtitle"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/_10sdp"
                android:layout_weight="10"
                android:fontFamily="@font/antipastopro_medium_trial"
                android:text="@string/lbl_profile"
                android:textColor="@color/colorBlack"
                android:textSize="@dimen/_16ssp" />

            <LinearLayout
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:orientation="vertical"
                android:padding="@dimen/_5sdp">

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:gravity="center"
                    android:orientation="horizontal">

                    <TextView
                        android:id="@+id/tvUserName"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:layout_weight="1"
                        android:fontFamily="@font/antipastopro_medium_trial"
                        android:gravity="center"
                        android:text="@string/lbl_profile"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_13ssp" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/iv_edit"
                        android:layout_width="@dimen/_24sdp"
                        android:layout_height="@dimen/_24sdp"
                        android:padding="@dimen/_5sdp"
                        android:src="@drawable/edit" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tvUserNumber"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/antipastopro_medium_trial"
                    android:text="@string/lbl_profile"
                    android:textColor="@color/colorBlack"
                    android:textSize="@dimen/_14ssp" />
            </LinearLayout>
        </LinearLayout>


    </androidx.appcompat.widget.Toolbar>


    <FrameLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ScrollView
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:fillViewport="true">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:orientation="vertical"
                android:paddingBottom="120dp">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_60sdp"
                    android:orientation="vertical"

                    >

                    <androidx.viewpager.widget.ViewPager
                        android:id="@+id/sliderviewPager"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_margin="@dimen/_5sdp"

                        />



                </LinearLayout>

          <LinearLayout
              android:id="@+id/linearMyBusiness"
              android:layout_width="match_parent"
              android:layout_height="@dimen/_50sdp"
              android:layout_marginTop="@dimen/_5sdp"
              android:layout_marginLeft="@dimen/_10sdp"
              android:layout_marginRight="@dimen/_10sdp"
              android:background="@drawable/edit_text_border"
              >

              <View
                  android:layout_width="@dimen/_5sdp"
                  android:layout_height="@dimen/_30sdp"
                  android:background="@color/teal_500"

                  android:layout_gravity="center"
                  />

              <TextView
                  android:layout_width="wrap_content"
                  android:layout_height="wrap_content"
                  android:layout_marginLeft="@dimen/_10sdp"
                  android:layout_weight="10"
                  android:layout_gravity="center"
                  android:fontFamily="@font/antipastopro_medium_trial"
                  android:text="@string/lbl_my_business"
                  android:textColor="@color/colorBlack"
                  android:textSize="@dimen/_18ssp" />

              <androidx.appcompat.widget.AppCompatImageView
                  android:layout_width="@dimen/_36sdp"
                  android:layout_height="@dimen/_36sdp"
                  android:padding="@dimen/_7sdp"
                  android:layout_marginRight="@dimen/_5sdp"
                  android:layout_gravity="center"
                  android:src="@drawable/business_bag" />

          </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearMyPost"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_50sdp"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:background="@drawable/edit_text_border"
                    >

                    <View
                        android:layout_width="@dimen/_5sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:background="@color/teal_500"

                        android:layout_gravity="center"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_weight="10"
                        android:layout_gravity="center"
                        android:fontFamily="@font/antipastopro_medium_trial"
                        android:text="@string/lbl_my_post"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_18ssp" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="@dimen/_36sdp"
                        android:layout_height="@dimen/_36sdp"
                        android:padding="@dimen/_7sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:layout_gravity="center"
                        android:src="@drawable/mypost" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linearShareus"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_50sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:background="@drawable/edit_text_border"
                    >

                    <View
                        android:layout_width="@dimen/_5sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:background="@color/teal_500"

                        android:layout_gravity="center"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_weight="10"
                        android:layout_gravity="center"
                        android:fontFamily="@font/antipastopro_medium_trial"
                        android:text="@string/lbl_share_us"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_18ssp" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="@dimen/_36sdp"
                        android:layout_height="@dimen/_36sdp"
                        android:padding="@dimen/_7sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:layout_gravity="center"
                        android:src="@drawable/share" />

                </LinearLayout>
                <LinearLayout
                    android:id="@+id/linearRateus"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/_50sdp"
                    android:layout_marginLeft="@dimen/_10sdp"
                    android:layout_marginRight="@dimen/_10sdp"
                    android:layout_marginTop="@dimen/_5sdp"
                    android:background="@drawable/edit_text_border"
                    >

                    <View
                        android:layout_width="@dimen/_5sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:background="@color/teal_500"

                        android:layout_gravity="center"
                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="@dimen/_10sdp"
                        android:layout_weight="10"
                        android:layout_gravity="center"
                        android:fontFamily="@font/antipastopro_medium_trial"
                        android:text="@string/lbl_rate_us"
                        android:textColor="@color/colorBlack"
                        android:textSize="@dimen/_18ssp" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="@dimen/_36sdp"
                        android:layout_height="@dimen/_36sdp"
                        android:padding="@dimen/_7sdp"
                        android:layout_marginRight="@dimen/_5sdp"
                        android:layout_gravity="center"
                        android:src="@drawable/rate_us" />

                </LinearLayout>



                <TextView
                    android:id="@+id/tvLogout"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:padding="@dimen/_10sdp"
                    android:background="@drawable/bg_logout"
                    android:fontFamily="@font/antipastopro_medium_trial"
                    android:text="@string/lbl_logout"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:textColor="@color/colorWhite"
                    android:textStyle="bold"
                    android:textSize="@dimen/_18ssp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/antipastopro_medium_trial"
                    android:text="@string/lbl_privacy_policy"
                    android:layout_marginTop="@dimen/_15sdp"
                    android:textSize="@dimen/_12ssp" />

                <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center"
                    android:fontFamily="@font/antipastopro_medium_trial"
                    android:text="@string/lbl_terms_services"
                    android:layout_marginTop="@dimen/_8sdp"
                    android:textSize="@dimen/_12ssp" />

                <LinearLayout
                    android:id="@+id/linearHelpSupport"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="right|bottom|center"
                    android:orientation="vertical"
                    android:gravity="right|bottom|center"
                    android:padding="@dimen/_10sdp"
                    >

                    <ImageView
                        android:id="@+id/ivWhatsapp"
                        android:layout_width="@dimen/_44sdp"
                        android:layout_height="@dimen/_44sdp"
                        android:gravity="center"
                        android:layout_gravity="center"
                        android:src="@drawable/support"

                        />

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:fontFamily="@font/antipastopro_medium_trial"
                        android:text="@string/lbl_help_support"
                        android:textSize="@dimen/_13ssp"/>



                </LinearLayout>

            </LinearLayout>




        </ScrollView>
    </FrameLayout>
</LinearLayout>